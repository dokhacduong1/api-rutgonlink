        .card(style='width: 38rem;border:none !important;').col-xl-12.col-lg-12.col-md-12.col-sm-12.col-12
            .card-body.text-center(id = ip style='padding: 50px;border-radius: 10px;box-shadow: 0 0 10px 0 rgba(0,0,0,.2)')
